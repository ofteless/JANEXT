{
  "name": "janext",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Adds seed=10 to OpenRouter chat requests",
  "permissions": ["scripting", "activeTab", "tabs", "storage"],
  "host_permissions": [
    "https://openrouter.ai/*",
    "https://*.janitorai.com/*"
  ],
    "background": {
    "service_worker": "service-worker.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.janitorai.com/*"],
      "js": ["content-script.js"],
      "run_at": "document_start"
    }
  ],
  "action": {
  "default_popup": "popup.html"
}
}